[run]
relative_files = True
source = .
branch = True
omit =
    tests/*
    */tests/*
    test_*.py
    *_test.py
    integration_tests/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    pass
    ...
